{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "TachFileTo",
    "version": "0.1.0",
    "identifier": "com.tachfileto.app",
    "build": {
        "beforeDevCommand": "",
        "devUrl": "http://localhost:3000",
        "beforeBuildCommand": "",
        "frontendDist": "../dist"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
